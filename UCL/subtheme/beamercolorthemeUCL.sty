\setbeamercolor*{titlelike}{parent=palette primary}

\setbeamercolor*{separation line}{}
\setbeamercolor*{fine separation line}{}

\setbeamercolor{block title}{use=structure,fg=UCLWhite,bg=UCLDarkPurple}
\setbeamercolor{block title alerted}{use=alerted text,fg=UCLWhite,bg=alerted text.fg!75!UCLBlack}
\setbeamercolor{block title example}{use=example text,fg=UCLWhite,bg=example text.fg!75!UCLBlack}

\setbeamercolor{block body}{parent=normal text,use=block title,bg=block title.bg!10!bg}
